<%- include('../header.ejs')%>



<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

<h1>Display</h1>
<div class="container">
    <div class="table-responsive">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Mobile Number</th>
                    <th>Address</th>
                    <th>Gender</th>
                    <th>Qualification</th>
                    <th>Course</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td><%= d._id %></td>
                    <td><%= d.name %></td>
                    <td><%= d.email %></td>
                    <td><%= d.number %></td>
                    <td><%= d.address %></td>
                    <td><%= d.gender %></td>
                    <td><%= d.qualification %></td>
                    <td><%= d.course %></td>


                </tr>
            </tbody>
        </table>
    </div>
</div>
